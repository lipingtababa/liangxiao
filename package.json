{
  "name": "synthetic-coding-team",
  "version": "0.1.0",
  "description": "AI coding agent system with GitHub webhook processing",
  "private": true,
  "workspaces": [
    "services/agent",
    "services/poller"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev:agent": "cd services/agent && npm run dev",
    "dev:poller": "cd services/poller && npm run dev",
    "build": "npm run build --workspaces",
    "install:all": "npm install --workspaces",
    "typecheck": "npm run typecheck --workspaces",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/synthetic-coding-team.git"
  },
  "keywords": [
    "ai",
    "agents",
    "coding",
    "automation",
    "github",
    "typescript"
  ],
  "author": "AI Team Leader",
  "license": "MIT"
}